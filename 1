class Animal{
	void animalSound() {
		System.out.println();}
}
class Pig extends Animal{
	void animalSound() {
		System.out.println("oinks oinks oinks oinks oinks oinks oinks"); }
}
class Cat extends Animal{
		void animalSound() {
			System.out.println("meows meows meows meows meows meows meows"); }
		}
		
class Dog extends Animal{
			void animalSound() {System.out.println("barks barks barks barks barks barks barks"); }
}
public class StreetAnimal {

public static void main(String[] args) {
		Pig P1 = new Pig();
		Cat C1 = new Cat();
		Dog D1 = new Dog();
		P1.animalSound();
		C1.animalSound();
		D1.animalSound();
		
	}
}
